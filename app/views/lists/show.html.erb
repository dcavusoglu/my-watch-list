<%= @list.name %>
<%= cl_image_tag @list.photo.key, height: 300, width: 400, crop: :fill %>
<br>

<ol>
<% @list.bookmarks.each do |bookmark| %>
  <li><%= bookmark.movie.title %> |
  <%= bookmark.comment %>
  <%= link_to "X", bookmark, method: :delete, data: { confirm: 'Are you sure?' } %></li>
<% end %>
</ol>
<br>


<div>
<%= link_to 'Back', lists_path, class: 'btn btn-info' %>
<%= link_to 'Add Bookmark', new_list_bookmark_path(@list), class: 'btn btn-success' %>
</div>
